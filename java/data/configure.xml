<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
">
    <bean id="team" class="Team">
        <property name="name" value="Norwich" />
        <property name="manager" ref="b" />
    </bean>
    
    <bean id="a" class="Manager">
        <property name="name" value="James Bond" />
        <property name="salary" value="47000" />
    </bean>
    
    <bean id="b" class="Manager">
        <property name="name" value="Ethan Hunt" />
        <property name="salary" value="46000" />
    </bean>
</beans>
